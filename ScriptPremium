--           AUTO BUY SEEDS OTIMIZADO (4 em 4 min)
-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•

local function AutoBuyAllSeeds()
    spawn(function()
        while getgenv().ZakzinSettings.AutoBuySeeds do
            pcall(function()
                print("ğŸŒ» Iniciando compra de seeds...")
                
                -- Lista dos RemoteEvents que podem comprar seeds
                local remoteNames = {
                    "BuyRow", "BuyItem", "GiftItem", "UseItem", 
                    "BuyGear", "BuyPortal", "BuyPlatform"
                }
                
                -- Lista completa das seeds
                local seeds = {
                    "Cactus Seed",
                    "Strawberry Seed",
                    "Pumpkin Seed",
                    "Sunflower Seed",
                    "Dragon Fruit Seed",
                    "Eggplant Seed",
                    "Watermelon Seed",
                    "Grape Seed",
                    "Cocotank Seed",
                    "Carnivorous Plant Seed",
                    "Mr. Carrot Seed",
                    "Tomatrio Seed",
                    "King Lemon Seed"
                }
                
                -- Tenta cada RemoteEvent com cada seed
                for _, remoteName in ipairs(remoteNames) do
                    local remote = ReplicatedStorage.Remotes:FindFirstChild(remoteName)
                    if remote and remote.ClassName == "RemoteEvent" then
                        for _, seed in ipairs(seeds) do
                            print("ğŸ›’ Comprando:", seed, "via", remoteName)
                            remote:FireServer(seed)
                            wait(0.2)
                        end
                    end
                end
                
                Rayfield:Notify({
                    Title = "âœ… Compra ConcluÃ­da!",
                    Content = "Todas as seeds foram compradas!",
                    Duration = 3
                })
                
                print("â° Aguardando 4 minutos para prÃ³xima compra...")
            end)
            
            -- Esperar 4 minutos (240 segundos)
            wait(240)
        end
    end)
end

-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
--                  ESP SYSTEMS
-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•

local function CreateBrainrotESP()
    local function AddHighlight(obj)
        if not obj:FindFirstChild("BrainrotESP") then
            local highlight = Instance.new("Highlight")
            highlight.Name = "BrainrotESP"
            highlight.FillColor = Color3.fromRGB(255, 0, 0)
            highlight.OutlineColor = Color3.fromRGB(255, 255, 255)
            highlight.FillTransparency = 0.5
            highlight.OutlineTransparency = 0
            highlight.Parent = obj
        end
    end
    
    spawn(function()
        while getgenv().ZakzinSettings.BrainrotESP do
            pcall(function()
                for _, obj in pairs(Workspace:GetDescendants()) do
                    if obj.Name:match("Brainrot") and obj:IsA("Model") then
                        AddHighlight(obj)
                    end
                end
            end)
            wait(2)
        end
        
        -- Remover ESPs ao desativar
        for _, obj in pairs(Workspace:GetDescendants()) do
            if obj.Name == "BrainrotESP" then
                obj:Destroy()
            end
        end
    end)
end

local function CreateSkeletonESP()
    local function AddBillboard(obj)
        if obj:FindFirstChild("Humanoid") and not obj:FindFirstChild("SkeletonESP") then
            local billboard = Instance.new("BillboardGui")
            billboard.Name = "SkeletonESP"
            billboard.Adornee = obj:FindFirstChild("HumanoidRootPart")
            billboard.Size = UDim2.new(0, 100, 0, 50)
            billboard.StudsOffset = Vector3.new(0, 3, 0)
            billboard.AlwaysOnTop = true
            
            local textLabel = Instance.new("TextLabel")
            textLabel.Size = UDim2.new(1, 0, 1, 0)
            textLabel.BackgroundTransparency = 1
            textLabel.Text = "ğŸ’€ " .. obj.Name
            textLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
            textLabel.TextStrokeTransparency = 0
            textLabel.Font = Enum.Font.SourceSansBold
            textLabel.TextSize = 14
            textLabel.Parent = billboard
            
            billboard.Parent = obj
        end
    end
    
    spawn(function()
        while getgenv().ZakzinSettings.SkeletonESP do
            pcall(function()
                for _, obj in pairs(Workspace:GetDescendants()) do
                    if obj.Name:match("Skeleton") or obj.Name:match("Zombie") and obj:IsA("Model") then
                        AddBillboard(obj)
                    end
                end
            end)
            wait(2)
        end
        
        -- Remover ESPs ao desativar
        for _, obj in pairs(Workspace:GetDescendants()) do
            if obj.Name == "SkeletonESP" then
                obj:Destroy()
            end
        end
    end)
end

-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
--              SISTEMA DE FLY (VOAR)
-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•

local flying = false
local flyConnection

local function StartFly()
    local Character = Player.Character
    if not Character then return end
    
    local HRP = Character:FindFirstChild("HumanoidRootPart")
    local Humanoid = Character:FindFirstChild("Humanoid")
    if not HRP or not Humanoid then return end
    
    flying = true
    
    local BodyVelocity = Instance.new("BodyVelocity")
    BodyVelocity.Velocity = Vector3.new(0, 0, 0)
    BodyVelocity.MaxForce = Vector3.new(9e9, 9e9, 9e9)
    BodyVelocity.Parent = HRP
    
    local BodyGyro = Instance.new("BodyGyro")
    BodyGyro.P = 9e4
    BodyGyro.MaxTorque = Vector3.new(9e9, 9e9, 9e9)
    BodyGyro.CFrame = HRP.CFrame
    BodyGyro.Parent = HRP
    
    flyConnection = RunService.Heartbeat:Connect(function()
        if not flying then
            BodyVelocity:Destroy()
            BodyGyro:Destroy()
            return
        end
        
        local moveDirection = Vector3.new(0, 0, 0)
        local Camera = workspace.CurrentCamera
        
        if UserInputService:IsKeyDown(Enum.KeyCode.W) then
            moveDirection = moveDirection + Camera.CFrame.LookVector
        end
        if UserInputService:IsKeyDown(Enum.KeyCode.S) then
            moveDirection = moveDirection - Camera.CFrame.LookVector
        end
        if UserInputService:IsKeyDown(Enum.KeyCode.A) then
            moveDirection = moveDirection - Camera.CFrame.RightVector
        end
        if UserInputService:IsKeyDown(Enum.KeyCode.D) then
            moveDirection = moveDirection + Camera.CFrame.RightVector
        end
        if UserInputService:IsKeyDown(Enum.KeyCode.Space) then
            moveDirection = moveDirection + Vector3.new(0, 1, 0)
        end
        if UserInputService:IsKeyDown(Enum.KeyCode.LeftShift) then
            moveDirection = moveDirection - Vector3.new(0, 1, 0)
        end
        
        BodyVelocity.Velocity = moveDirection * getgenv().ZakzinSettings.FlySpeed
        BodyGyro.CFrame = Camera.CFrame
    end)
end

local function StopFly()
    flying = false
    if flyConnection then
        flyConnection:Disconnect()
    end
end

